<div class="container" id="top">
    <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-light">
      <ul class="nav ">
        <li class="nav-item">
          <a class="nav-link active" href="./index.html">About</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="portfolio.html">Portfolio</a>
        </li>
        <li class="nav-item">
          <a class="nav-link disabled" href="./contact.html">Contact</a>
        </li>
      </ul>
    </nav>
  </div>

  <div class="articles">
    <div class="load"></div>
    {{#each_upto articles 8}}
        {{#unless saved}}
        <div class="article">
            <img src="{{image}}" style="height:100px;">
            <a href="{{link}}"><h2>{{title}}</h2></a>
            <p>{{summary}}</p>
            <button class="btn btn-primary save" data-id="{{_id}}">SAVE</button>
            <br>
        </div>
        {{/unless}}
    {{/each_upto}}
</div>